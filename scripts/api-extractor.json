
{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "<projectFolder>/lib/docs-entry.d.ts",
  "projectFolder": ".",
  "bundledPackages": [],

  "compiler": {
    "tsconfigFilePath": "../../tsconfig.json"
  },
  "apiReport": {
    "enabled": true,
    "reportFolder": "temp/"
  },
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "input/lasens.api.json"
  },
  "dtsRollup": {
    "enabled": true
  },
  "tsdocMetadata": {
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "none"
      }

    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    }
  }
}
