{
  "name": "@alaq/nucl",
  "version": "6.0.0-alpha.1",
  "description": "Enhanced reactive primitive with plugin system - built on Quark",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./nucleus": {
      "types": "./types/nucleus/index.d.ts",
      "import": "./dist/nucleus/index.js"
    },
    "./fusion": {
      "types": "./types/fusion/index.d.ts",
      "import": "./dist/fusion/index.js"
    },
    "./heavy": {
      "types": "./types/heavy/index.d.ts",
      "import": "./dist/heavy/index.js"
    }
  },
  "files": [
    "dist",
    "types",
    "src"
  ],
  "scripts": {
    "build": "bun run ../../scripts/index.ts build",
    "test": "bun test",
    "dev": "bun test --watch",
    "bench": "bun benchmark/run.ts",
    "bench:browser": "bun benchmark/build-browser.ts && bun benchmark/server.ts"
  },
  "keywords": [
    "reactive",
    "state",
    "nucl",
    "quark",
    "computed",
    "fusion"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/bun": "latest",
    "@alaq/quark": "workspace:*"
  },
  "peerDependencies": {
    "@alaq/quark": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alaq/nucl"
  }
}
