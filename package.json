    "@types/tap": "^15.0.8",
    "chokidar": "^3.5.3",
    "colorette": "^2.0.19",
    "fs-extra": "^11.1.1",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "prettier": "2.8.7",
    "tap": "^16.3.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.0",
    "tap-mocha-reporter":"^5.0.3",
    "type-fest": "^3.8.0",
